{
  "manifest_version": 2,
  "name": "MDNLocalDev",
  "description": "Connect local front end to MDN.",
  "version": "0.0.1",
  "author": "Florian Dieminger",
  "homepage_url": "https://github.com/fiji-flo/mdnlocaldev",
  "icons": {
    "64": "icons/icon.svg"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://developer.allizom.org/",
    "https://developer.mozilla.org/"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {
      "64": "icons/icon-grey.svg"
    },
    "default_title": "MDNLocalDev"
  }
}